<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpaper"
    tools:context=".homeUser">

    <ImageButton
        android:id="@+id/detail"
        android:layout_width="107dp"
        android:layout_height="109dp"
        android:layout_below="@+id/booking"
        android:layout_alignParentStart="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="83dp"
        android:background="@drawable/detail" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/detail"
        android:layout_alignEnd="@+id/detail"
        android:layout_alignParentStart="true"
        android:layout_marginStart="69dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="15dp"
        android:text="รายละเอียด"
        android:textSize="16sp" />

    <ImageButton
        android:id="@+id/checkqueue"
        android:layout_width="107dp"
        android:layout_height="109dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="58dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="54dp"
        android:layout_toEndOf="@+id/booking"
        android:background="@drawable/checkqueue" />

    <ImageButton
        android:id="@+id/btnuser"
        android:layout_width="106dp"
        android:layout_height="113dp"
        android:layout_below="@+id/checkqueue"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="67dp"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="48dp"
        android:layout_toEndOf="@+id/detail"
        android:background="@drawable/korn" />

    <TextView
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkqueue"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="255dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="81dp"
        android:text="เช็คคิว"
        android:textSize="16sp" />

    <TextView
        android:layout_width="101dp"
        android:layout_height="21dp"
        android:layout_below="@+id/btnuser"
        android:layout_alignStart="@+id/btnuser"
        android:layout_alignEnd="@+id/btnuser"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="-7dp"
        android:text="แก้ไขโปรไฟล์"
        android:textSize="16sp" />

    <ImageButton
        android:id="@+id/booking"
        android:layout_width="107dp"
        android:layout_height="109dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="58dp"
        android:layout_marginTop="87dp"
        android:background="@drawable/booking" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/booking"
        android:layout_alignParentStart="true"
        android:layout_marginStart="82dp"
        android:layout_marginTop="9dp"
        android:text="จองคิว"
        android:textSize="16sp" />

</RelativeLayout>